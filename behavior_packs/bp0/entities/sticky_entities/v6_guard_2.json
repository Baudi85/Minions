{
    "format_version": "1.16.0",
    "minecraft:entity": {
        "description": {
            "identifier": "cyd:v6_guard_2",
            "is_spawnable": false,
            "is_summonable": true,
            "is_experimental": false,
            "animations": {
                "honey_ctrl": "controller.animation.trigger.guard.rm_honey",
                "rm_honey": "animation.rm_honey",
                "rm_hay": "animation.rm_hay",
                "snd_ctrl": "controller.animation.sound.guard.ctrl",
                "peripheral_ctrl": "controller.animation.trigger.guard.peripheral",
                "peripheral": "animation.guard.peripheral",
                "caught_dialogue": "controller.animation.sound.guard.caught.dialogue.ctrl",
                "question_dialogue": "controller.animation.sound.guard.question.dialogue.ctrl"
            },
            "scripts": {
                "animate": [
                    "honey_ctrl",
                    "snd_ctrl",
                    "peripheral_ctrl",
                    "caught_dialogue",
                    "question_dialogue"
                ]
            }
        },
        "component_groups": {
            "cyd:despawn":{
                "minecraft:instant_despawn":{}
            },
            "cyd:sid_0": {
                "minecraft:skin_id": {
                    "value": 0
                }
            },
            "cyd:sid_1": {
                "minecraft:skin_id": {
                    "value": 1
                }
            },
            "cyd:sid_2": {
                "minecraft:skin_id": {
                    "value": 2
                }
            },
            "cyd:mv_0": {
                "minecraft:mark_variant": {
                    "value": 0
                }
            },
            "cyd:mv_1": {
                "minecraft:mark_variant": {
                    "value": 1
                }
            },
            "cyd:mv_2": {
                "minecraft:mark_variant": {
                    "value": 2
                }
            },
            "cyd:targeting": {
                "minecraft:behavior.nearest_attackable_target": {
                    "priority": 2,
                    "within_radius": 5,
                    "reselect_targets": true,
                    "entity_types": [
                        {
                            "filters": {
                                "any_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "value": "target_box"
                                    }
                                ]
                            },
                            "max_dist": 20
                        }
                    ],
                    "must_see": true,
                    "must_see_forget_duration": 6
                },
                "minecraft:behavior.ranged_attack": {
                    "attack_interval_min": 1,
                    "attack_interval_max": 1,
                    "charge_shoot_trigger": 0.5,
                    "charge_charged_trigger": 0.1,
                    "attack_radius": 2
                }
            },
            "cyd:ignited": {
                "minecraft:is_ignited": {}
            }
        },
        "components": {
            "minecraft:collision_box": {
                "width": 0.5,
                "height": 2
            },
            "minecraft:type_family": {
                "family": [
                    "human",
                    "v6_guard",
                    "no_stick"
                ]
            },
            "minecraft:physics": {},
            "minecraft:damage_sensor": {
                "triggers": [
                    {
                        "deals_damage": false,
                        "on_damage": {
                            "filters": {
                                "all_of": [
                                    {
                                        "test": "is_family",
                                        "subject": "other",
                                        "operator": "==",
                                        "value": "player"
                                    }
                                ]
                            },
                            "event": "evt:sid_1"
                        }
                    }
                ]
            },
            "minecraft:attack": {
                "value": 0
            },
            "minecraft:navigation.generic": {
                "can_path_over_water": true,
                "avoid_damage_blocks": true
            },
            "minecraft:movement.basic": {},
            "minecraft:jump.static": {},
            "minecraft:can_climb": {},
            "minecraft:movement": {
                "value": 0.31
            },
            "minecraft:environment_sensor": {
                "triggers": [
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "distance_to_nearest_player",
                                    "operator": "<=",
                                    "value": 17
                                },
                                {
                                    "test": "has_component",
                                    "operator": "not",
                                    "value": "minecraft:is_ignited"
                                }
                            ]
                        },
                        "event": "evt:ignite"
                    },
                    {
                        "filters": {
                            "all_of": [
                                {
                                    "test": "distance_to_nearest_player",
                                    "operator": ">=",
                                    "value": 18
                                },
                                {
                                    "test": "has_component",
                                    "operator": "equals",
                                    "value": "minecraft:is_ignited"
                                }
                            ]
                        },
                        "event": "evt:rm_ignite"
                    }
                ]
            }
        },
        "events": {
            "minecraft:entity_spawned": {
                "add": {
                    "component_groups": [
                        "cyd:targeting"
                    ]
                }
            },
            "evt:rm_targeting": {
                "remove": {
                    "component_groups": [
                        "cyd:targeting"
                    ]
                }
            },
            "evt:mv_0": {
                "add": {
                    "component_groups": [
                        "cyd:mv_0"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "cyd:mv_1",
                        "cyd:mv_2"
                    ]
                }
            },
            "evt:mv_1": {
                "add": {
                    "component_groups": [
                        "cyd:mv_1"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "cyd:mv_0",
                        "cyd:mv_2"
                    ]
                }
            },
            "evt:mv_2": {
                "add": {
                    "component_groups": [
                        "cyd:mv_2"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "cyd:mv_1",
                        "cyd:mv_0"
                    ]
                }
            },
            "evt:sid_0": {
                "add": {
                    "component_groups": [
                        "cyd:sid_0"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "cyd:sid_1",
                        "cyd:sid_2"
                    ]
                }
            },
            "evt:sid_1": {
                "add": {
                    "component_groups": [
                        "cyd:sid_1"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "cyd:sid_0",
                        "cyd:sid_2"
                    ]
                }
            },
            "evt:sid_2": {
                "add": {
                    "component_groups": [
                        "cyd:sid_2"
                    ]
                },
                "remove": {
                    "component_groups": [
                        "cyd:sid_0",
                        "cyd:sid_1",
                        "cyd:sid_2"
                    ]
                }
            },
            "evt:ignite": {
                "add": {
                    "component_groups": [
                        "cyd:ignited"
                    ]
                }
            },
            "evt:rm_ignite": {
                "remove": {
                    "component_groups": [
                        "cyd:ignited"
                    ]
                }
            },
            "evt:despawn":{
                "add":{
                    "component_groups":[
                        "cyd:despawn"
                    ]
                }
            }
        }
    }
}